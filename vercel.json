{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "framework":"create-react-app",
  "rewrites":[
    {
    "source":"/api/(.*)",
    "destination":"/server/server.js"
  }
  ],
  "builds":[
    {
    "src":"./server/server.js",
    "use":"@vercel/node"
    },
    {
      "src":"./client/build",
      "use":"@vercel/static"
    }
  ],
}
